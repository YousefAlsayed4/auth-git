{
    "name": "acmei/auth-package",
    "description": "A Laravel package for login and registration pages",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "yousef alsayed"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "illuminate/support": "^10.0|^11.0",
        "spatie/ignition": "^1.15",
        "symfony/console": "^6.2.3|^7.0",
        "symfony/var-dumper": "^6.2.3|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Acme\\AuthPackage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Acme\\AuthPackage\\AuthPackageServiceProvider"
            ]
        }
    }
}
